---
import PageLayout from '~/layouts/PageLayout.astro';
import Hero from '~/components/widgets/Hero.astro';

import client from '~/data/client/client.json';
---

<PageLayout>
	<Hero />
	<h1 class="text-red-500 z-50">{client.companyName}</h1>
	<h1 class="text-red-500 z-50">{client.email}</h1>
	<h1 class="text-red-500 z-50">{client.phone}</h1>
	<h1 class="text-red-500 z-50">{client.phoneDisplay}</h1>
	<h1 class="text-red-500 z-50">{client.tagline}</h1>
</PageLayout>
